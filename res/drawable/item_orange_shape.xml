<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape android:shape="rectangle" >
            <corners android:radius="5dp" />

            <solid android:color="#80A35528" />

            <stroke
                android:width="1dp"
                android:color="#A35528" />
        </shape>
    </item>

</layer-list>